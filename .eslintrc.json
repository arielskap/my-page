{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended"
    ],
    "parser": "babel-eslint",
    "plugins": ["react", "jsx-a11y", "import", "react-hooks"],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "quotes": [2, "single", {
            "avoidEscape": true
        }],
        "react/button-has-type": 2
    }
}